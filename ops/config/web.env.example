# Example environment configuration for LaBot Web
# Copy to /etc/labot/web.env and update with actual values

ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://localhost:5000

# Database
ConnectionStrings__DefaultConnection="Host=localhost;Database=labot;Username=labot;Password=CHANGE_ME_SECURE_PASSWORD"

# Stripe
Stripe__SecretKey="sk_live_YOUR_SECRET_KEY_HERE"
Stripe__PublishableKey="pk_live_YOUR_PUBLISHABLE_KEY_HERE"
Stripe__WebhookSecret="whsec_YOUR_WEBHOOK_SECRET_HERE"
Stripe__BasicPriceId="price_YOUR_BASIC_PRICE_ID"
Stripe__ProPriceId="price_YOUR_PRO_PRICE_ID"

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft_AspNetCore=Warning
